;; Some inefficient arithmetic computations for benchmarking.

;; Compute n(n+1)/2 with a non tail-recursive call.

(def! sumdown
  (fn* [n]                              
    (if (= n 0)
      0
      (+ n (sumdown  (- n 1))))))

;; Compute a Fibonacci number with two recursions.

(def! fib
  (fn* [n]                              
    (if (<= n 1)
      n
      (+ (fib (- n 1)) (fib (- n 2))))))
